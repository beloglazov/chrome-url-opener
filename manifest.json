{
  "name": "Favorite URL Opener",
  "version": "1.0",
  "manifest_version": 2,
  "description": "This extension allows you to save several URLs and open them with one click!",

  "icons": { 
    "16": "multiple16.png",
    "48": "multiple48.png"
  },

  "background": { "scripts": ["background.js"] },
  "browser_action": {
    "default_icon": "multiple19.png",
    "default_title": "Open my URLs"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs"
  ]
}